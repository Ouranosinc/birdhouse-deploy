{
  "type": "object",
  "required": [
    "url",
    "name",
    "type",
    "documentation",
    "description"
  ],
  "properties": {
    "href": {
      "type": "string",
      "format": "uri",
      "pattern": "^https?://"
    },
    "name": {
      "type": "string",
      "minLength": 1
    },
    "type": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string"
      }
    },
    "documentation": {
      "type": "string",
      "format": "uri",
      "pattern": "^https?://"
    },
    "description": {
      "type": "string",
      "minLength": 1
    }
  }
}